<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-benas-3.0.xsd">
    <bean id="connectionMaker" class="com.study.usermanagementapp.dao.MConnectionMaker" />
    <bean id="userDao" class="com.study.usermanagementapp.dao.UserDao">
        <property name="connectionMaker" ref="connectionMaker" />
    </bean>

</beans>